<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="staff02" class="com.spring.homework.week05.springframework.po.Staff" scope="prototype">
        <property name="id" value="2"></property>
        <property name="name" value="staff02"></property>
        <property name="age" value="18"></property>
    </bean>

    <bean id="staff03" class="com.spring.homework.week05.springframework.po.Staff" scope="prototype">
        <constructor-arg name="id" value="3"></constructor-arg>
        <constructor-arg name="name" value="staff3"></constructor-arg>
        <constructor-arg name="age" value="23"></constructor-arg>
    </bean>
</beans>